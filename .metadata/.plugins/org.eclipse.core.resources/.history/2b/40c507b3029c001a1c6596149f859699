package org.boot.az.bean;

/**
 * 
 * @author 作者 : 阿铮
 * 
 * @version 创建时间：2019年1月18日 下午6:02:04
 * 
 *          类说明 点击类型
 * 
 */
public class ViewButton extends Button {

	private String url;// 网页链接

	
	public ViewButton() {
		super();
		this.setType("view");
	}
	

	public ViewButton(String url) {
		super();
		this.url = url;
		this.setType("view");
	}
	
	public ViewButton(String name,String url) {
		super();
		this.url = url;
		this.setName(name);
		this.setType("view");
	}
	
	
	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

}

package org.boot.az.bean;

/**
 * 
 * @author 作者 : 阿铮
 * 
 * @version 创建时间：2019年1月25日 下午1:11:04
 * 
 *          类说明  点击拍照
 * 
 */
public class PhotoOrAlbumButton extends Button {


	private String key;

	public PhotoOrAlbumButton() {
		
	}
	
	public PhotoOrAlbumButton(String name, String key) {
		super();
		this.setName(name);
		this.setType("pic_photo_or_album");
		this.key = key;
	}
	
	
	public PhotoOrAlbumButton(String name, Button[] sub_button, String key) {
		super(name, sub_button);
		this.key = key;
	}

	public String getKey() {
		return key;
	}

	public void setKey(String key) {
		this.key = key;
	}

	
	


}
